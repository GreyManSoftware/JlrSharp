<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <RootNamespace>JlrSharp</RootNamespace>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Authors>Chris Davies</Authors>
    <Company>GreyMan Software</Company>
    <Description>A C# wrapper for the Jaguar Remote (InControl) services. This allows you to sign into your account and use functions such as remote engine start, grab service information and much more!</Description>
    <Copyright>GreyMan Software 2020</Copyright>
    <PackageProjectUrl>https://github.com/GreyManSoftware/JlrSharp</PackageProjectUrl>
    <RepositoryUrl>https://github.com/GreyManSoftware/JlrSharp</RepositoryUrl>
    <PackageTags>Jaguar, Remote, InControl</PackageTags>
    <PackageReleaseNotes></PackageReleaseNotes>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageIcon>jaguarApiLogo.PNG</PackageIcon>
    <PackageIconUrl />
    <Version>1.0.10</Version>
    </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)' == 'Release'">
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <None Remove=".gitignore" />
    <None Include="jaguarApiLogo.PNG">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
    <PackageReference Include="RestSharp" Version="106.6.10" />
  </ItemGroup>

</Project>